  prizm.PrizmBegin();
}

void loop(){

}
//Blink green LED
void GreenLED(){
  prizm.setGreenLED(HIGH)
  delay(250);
  prim.setGreenLED(LOW)
  delay(250);
}

//Pulse Motor
void PulseMotor(){
  prizm.setMotorPower(1,50);
  delay(1000);
  prizm.setMotorPower(1,0);
  delay(0);
}
void SweepServo(){
  prizm.setServoPosition(1,100);
  delay(500);
  prizm.setServoPosition(1,100);
  delay(500);
}
